//realizar un programa el cual solicite su nombre y apellidos*/
Proceso nombreApellido
	Definir nombre, apellido Como Caracter;
	
	
	Escribir "¿Cual es tú nombre?";
	Leer nombre;
	
	Escribir "¿Cual es tu apellido?";
	Leer apellido;
	
	Limpiar Pantalla;
	Escribir "Tú nombre completo es ", nombre, " ", apellido;
	
	
	
	
FinProceso
